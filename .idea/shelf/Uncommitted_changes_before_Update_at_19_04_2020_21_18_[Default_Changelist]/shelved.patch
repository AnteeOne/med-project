<+>/*some css*/\r\n#cab-text {\r\n    font-style: normal;\r\n    font-weight: 300;\r\n    font-size: 30px;\r\n    line-height: 42px;\r\n}\r\n\r\n.h_nav_container {\r\n    height: 43px;\r\n    width: 218px;\r\n}\r\n\r\n.my-container {\r\n    width: 600px;\r\n    margin: auto;\r\n    padding-top: 40px;\r\n    padding-bottom: 40px;\r\n}\r\n\r\n.img-place {\r\n    border-radius: 150px;\r\n    width: 200px;\r\n    height: 200px;\r\n    padding-top: 50px;\r\n    border: 2px solid #000000;\r\n    float: left;\r\n}\r\n\r\n.user {\r\n    height: 500px;\r\n    border-bottom: 1px solid lightgray;\r\n}\r\n\r\n.user-data {\r\n    float: right;\r\n    padding-right: 10px;\r\n    font-size: 22px;\r\n}\r\n\r\n.row1, .row4, .row5, .row6, .row7 {\r\n    display: flex;\r\n}\r\n\r\n.row2 {\r\n    padding-top: 15px;\r\n    font-weight: bold;\r\n}\r\n\r\n.row3 {\r\n    padding-top: 15px;\r\n}\r\n\r\n.user-data div:first-child {\r\n    padding-right: 50px;\r\n}\r\n\r\n.row p:first-child {\r\n    padding-right: 50px;\r\n}\r\n\r\n.row {\r\n    padding-left: 15px;\r\n}\r\n\r\n.row p:last-child {\r\n    font-weight: bold;\r\n}\r\n\r\n.name p:last-child {\r\n    font-weight: bold;\r\n}\r\n\r\n.soname p:last-child {\r\n    font-weight: bold;\r\n}\r\n\r\n.stat-title {\r\n    font-size: 22px;\r\n    text-align: center;\r\n}\r\n.statistics {\r\n    display: flex;\r\n    justify-content: center;\r\n    padding-top: 10px;\r\n}\r\n\r\n.statistics :last-child {\r\n    padding-left: 20px;\r\n}\r\n\r\n#dropdownMenuButton {\r\n    border-radius: 10px;\r\n    padding-bottom: 8px;\r\n    background-color: #b4b9c2;\r\n}\r\n\r\n.pers-data {\r\n    width: 100px;\r\n    background-color: #e9e9ea;\r\n    border-radius: 10px;\r\n}\r\n\r\n#pers-Data {\r\n    width: 200px;\r\n}\r\n\r\n.inner-text {\r\n    text-align: center;\r\n    padding-top: 20px;\r\n}\r\n\r\n.attention {\r\n    font-size: 15px;\r\n    margin-top: 20px;\r\n    font-weight: bold;\r\n}\r\n/*----------*/\r\n\r\n/* Style The Dropdown Button */\r\n.dropbtn {\r\n    background-color: #b4b9c2;\r\n    color: white;\r\n    padding: 7px;\r\n    padding-bottom: 9px;\r\n    font-size: 16px;\r\n    border: none;\r\n    cursor: pointer;\r\n    border-radius: 5px;\r\n}\r\n\r\n/* The container <div> - needed to position the dropdown content */\r\n.dropdown {\r\n    position: relative;\r\n    display: inline-block;\r\n}\r\n\r\n/* Dropdown Content (Hidden by Default) */\r\n.dropdown-content {\r\n    display: none;\r\n    position: absolute;\r\n    background-color: #f9f9f9;\r\n    min-width: 160px;\r\n    box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);\r\n    z-index: 1;\r\n    padding-right: 15px;\r\n}\r\n\r\n/* Links inside the dropdown */\r\n.dropdown-content a {\r\n    color: black;\r\n    padding: 12px 16px;\r\n    text-decoration: none;\r\n    display: block;\r\n}\r\n\r\n/* Change color of dropdown links on hover */\r\n.dropdown-content a:hover {\r\n    background-color: #f1f1f1\r\n}\r\n\r\n/* Show the dropdown menu on hover */\r\n.dropdown:hover .dropdown-content {\r\n    display: block;\r\n}\r\n\r\n/* Change the background color of the dropdown button when the dropdown content is shown */\r\n.dropdown:hover .dropbtn {\r\n    background-color: #4e555b;\r\n}
